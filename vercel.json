{
  "version": 2,
  "builds": [
    { "src": "html/**/*.html", "use": "@vercel/static" },
    { "src": "css/**/*", "use": "@vercel/static" },
    { "src": "js/**/*", "use": "@vercel/static" },
    { "src": "imgs/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/", "dest": "/html/index.html" },
    { "src": "/(.*)", "dest": "/html/$1" }
  ]
}
